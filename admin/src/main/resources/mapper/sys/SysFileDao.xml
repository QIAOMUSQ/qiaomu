<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qiaomu.modules.sys.dao.SysFileDao">


    <insert id="insertInfo" parameterType="com.qiaomu.modules.sys.entity.SysFileEntity" useGeneratedKeys="true" keyProperty="id">

        insert into sys_file
        (PATH,NAME,DATE_TIME,ORG_NAME,FILE_SIZE,SERVICE_PATH)
         VALUES
		(#{path} ,#{name},#{dateTime},#{orgName},#{fileSize},#{servicePath})
    </insert>
</mapper>