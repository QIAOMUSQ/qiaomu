<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qiaomu.modules.propertycompany.dao.YwPropertyCompanyDao">
    <select id="findCompanyByUserId"
            resultType="com.qiaomu.modules.propertycompany.entity.YwPropertyCompany" parameterType="long">
        SELECT * FROM yw_property_company   where administrator_id = #{userId}
    </select>
</mapper>