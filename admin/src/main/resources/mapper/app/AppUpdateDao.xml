<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qiaomu.modules.sys.dao.AppUpdateDao">

    <insert id="addAppUpdateInfo" parameterType="com.qiaomu.modules.sys.entity.AppUpdateEntity">
        insert into pluto_updateAppinfo (appVersion,appUrl,updateType,createdAs,updatedAs) VALUES (#{appVersion},#{appUrl},#{updateType},#{createdAs},#{updatedAs})
    </insert>

    <select id="getMaxVersion" resultType="com.qiaomu.modules.sys.entity.AppUpdateEntity">
        SELECT * FROM pluto_updateAppinfo ORDER BY appVersion DESC LIMIT 1
    </select>


</mapper>