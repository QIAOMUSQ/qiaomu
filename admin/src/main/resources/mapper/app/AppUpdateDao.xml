<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qiaomu.modules.app.dao.AppUpdateDao">

    <insert id="addAppUpdateInfo" parameterType="com.qiaomu.modules.app.entity.AppUpdateEntity">
        insert into pluto_updateAppinfo (appVersion,appUrl,updateType,clientType,createdAs,updatedAs) VALUES (#{appVersion},#{appUrl},#{updateType},#{clientType},#{createdAs},#{updatedAs})
    </insert>

    <select id="getMaxVersion" resultType="com.qiaomu.modules.app.entity.AppUpdateEntity" parameterType="String">
        SELECT * FROM pluto_updateAppinfo where clientType=#{clientType} ORDER BY appVersion DESC, id DESC LIMIT 1
    </select>


</mapper>