<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qiaomu.push.dao.PushMessageDao">

    <select id="findByReceivePhone" resultType="com.qiaomu.push.entity.PushMessage">
        SELECT
          *
        FROM
            push_message_data
        <where>

            <if test="receivePhone !=null">
                receive_phone =#{receivePhone}
            </if>
            <if test="status != null">
               and status = #{status}
            </if>
        </where>
    </select>
</mapper>